{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "generators": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "api": "apps/api",
    "api-shared-abstract": "libs/api/shared/abstract",
    "api-shared-common": "libs/api/shared/common",
    "api-shared-decorator": "libs/api/shared/decorator",
    "api-shared-exception": "libs/api/shared/exception",
    "api-shared-filter": "libs/api/shared/filter",
    "api-shared-validation": "libs/api/shared/validation",
    "api-user": "libs/api/user",
    "shared-data-access-model": "libs/shared/data-access/model",
    "web": "apps/web",
    "web-e2e": "apps/web-e2e"
  },
  "defaultProject": "web"
}
